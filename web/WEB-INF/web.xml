<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
    <servlet>
        <servlet-name>SignUpServlet</servlet-name>
        <servlet-class>com.servlet.SignUpServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignUpServlet</servlet-name>
        <url-pattern>/SignUpServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SignInServlet</servlet-name>
        <servlet-class>com.servlet.SignInServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SignInServlet</servlet-name>
        <url-pattern>/SignInServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MemberManagerServlet</servlet-name>
        <servlet-class>com.servlet.MemberManagerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MemberManagerServlet</servlet-name>
        <url-pattern>/MemberManagerServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>FindEditMemberServlet</servlet-name>
        <servlet-class>com.servlet.FindEditMemberServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FindEditMemberServlet</servlet-name>
        <url-pattern>/FindEditMemberServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>UpdateEditMemberServlet</servlet-name>
        <servlet-class>com.servlet.UpdateEditMemberServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpdateEditMemberServlet</servlet-name>
        <url-pattern>/UpdateEditMemberServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>MySignInServlet</servlet-name>
        <servlet-class>com.servlet.MySignInServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MySignInServlet</servlet-name>
        <url-pattern>/MySignInServlet</url-pattern>
    </servlet-mapping>
</web-app>
